<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
      PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
   "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="sample">
	<resultMap id="memberResult01" type="MemberVO">
		<result property="memId" column="memId"  />
		<result property="memPw" column="memPw" />
		<result property="memName" column="memName" />
	</resultMap>

	<select id="searchList" resultMap="memberResult01" parameterType="java.util.Map"> 
      <![CDATA[
         select memId, memPw, memName
         from   test_member
      ]]>
		<where>
			<if test=" memId != ''  and  memId != null">
				id = #{memId}
			</if>
		</where>
	</select>
	
	<insert id="insertMember" parameterType="java.util.Map">
		insert into test_member(memId, memPw, memName) values(#{memId}, #{memPw}, #{memName})
	</insert>	

</mapper>